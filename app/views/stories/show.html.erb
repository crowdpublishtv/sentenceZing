<center> 
  <div class="storyshow" data-story-id="<%=@story.id %>" >
    <h1><%= @story.title %></h1>
    <h2><%= @story.category %></h2>
    by <%= User.find(@story.user_id).name %> and 
    <% if !(User.find(@story.user2id).name == nil ) %>
      <%= User.find(@story.user2id).name %>
    <% end if %>
    <br/><br/>
  
    <% if @story.microposts.any? %>
      <%= render 'shared/feed' %>
    <% end %>
  
    <br/>
    <% @like = @story.likes.build %>
    <% @story = @story.id %>
    <%= render 'shared/likes_form' %>
  
    <br/> <br/>
  </div>
</center>
 